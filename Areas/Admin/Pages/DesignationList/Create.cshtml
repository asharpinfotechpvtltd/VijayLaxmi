@page
@model VijayLaxmi.Areas.Admin.Pages.DesignationList.CreateModel
@{
    ViewData["Title"] = "Create";
}
<h4>Add Designation</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="Designation.DesignationName" class="control-label">Designation</label>
                <input type="text" asp-for="Designation.DesignationName" class="form-control" required />
                <span asp-validation-for="Designation.DesignationName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {

}


